{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "targetDefaults": {
    "_build": {
      "cache": true,
      "inputs": ["{projectRoot}/src/**/*", "{projectRoot}/tsconfig.json", "{projectRoot}/.swcrc"],
      "outputs": ["{projectRoot}/dist"]
    },
    "_lint": {
      "cache": true,
      "inputs": [
        "{projectRoot}/src/**/*",
        "{projectRoot}/dev/**/*",
        "{projectRoot}/eslint.config.js"
      ]
    },
    "_typecheck": {
      "cache": true,
      "inputs": [
        "{projectRoot}/src/**/*",
        "{projectRoot}/dev/**/*",
        "{projectRoot}/tsconfig.json",
        "{projectRoot}/dev/tsconfig.json"
      ]
    },
    "_test": {
      "cache": true,
      "inputs": ["{projectRoot}/src/**/*", "{projectRoot}/dev/**/*"]
    },
    "_test:int": {
      "cache": true,
      "inputs": ["{projectRoot}/src/**/*", "{projectRoot}/dev/**/*"]
    },
    "_test:e2e": {
      "cache": true,
      "inputs": ["{projectRoot}/src/**/*", "{projectRoot}/dev/**/*"]
    },
    "_dev:build": {
      "cache": true,
      "inputs": ["{projectRoot}/src/**/*", "{projectRoot}/dev/**/*"],
      "outputs": ["{projectRoot}/dev/.next"]
    }
  },
  "defaultBase": "main"
}
