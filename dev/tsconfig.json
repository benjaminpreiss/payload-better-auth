{
  "extends": "../tsconfig.json",
  "exclude": [".next"],
  "include": [
    "**/*.js",
    "**/*.jsx",
    "**/*.mjs",
    "**/*.cjs",
    "**/*.ts",
    "**/*.tsx",
    "../src/**/*.ts",
    "../src/**/*.tsx",
    "next.config.mjs",
    ".next/types/**/*.ts"
  ],
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "bundler",
    "baseUrl": "./",
    "paths": {
      "@/*": ["./*"],
      "@payload-config": ["./payload.config.ts"],
      "payload-better-auth": ["../src/index.ts"],
      "payload-better-auth/client": ["../src/exports/client.ts"],
      "payload-better-auth/rsc": ["../src/exports/rsc.ts"]
    },
    "noEmit": true,
    "declaration": false,
    "emitDeclarationOnly": false,
    "composite": false,
    "incremental": false
  }
}
